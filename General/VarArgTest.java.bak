package com.nt.general;

public class VarArgTest{
public static void sum(int... x){
	int total=0;
	for(int i=0;i<x.length;i++){
           total=total+x[i];
	}//for
	System.out.println("Sum: "+total);
}//sum(...)
public static void main(String[] args) {
sum();
sum(10);
sum(19,434,434);
sum(424242,4242,2525,252);
	}//main
}//class
